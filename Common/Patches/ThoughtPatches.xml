<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<!-- Pessimistic antidepressant patch -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThoughtDef[defName="MoodOffsetPessimist"]/workerClass</xpath>
		<value>
			<workerClass>Psychology.ThoughtWorker_AlwaysActivePsychology</workerClass>
		</value>
	</Operation>

	<!-- Depressive antidepressant patch -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThoughtDef[defName="MoodOffsetDepressive"]/workerClass</xpath>
		<value>
			<workerClass>Psychology.ThoughtWorker_AlwaysActivePsychology</workerClass>
		</value>
	</Operation>

	<!-- Tortured Artist antidepressant patch -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThoughtDef[defName="MoodOffsetTorturedArtist"]/workerClass</xpath>
		<value>
			<workerClass>Psychology.ThoughtWorker_AlwaysActivePsychology</workerClass>
		</value>
	</Operation>

	<!-- Legacy code, keep for backwards compatibility -->
	<Operation Class="PatchOperationAdd">
		<xpath>*/ThoughtDef[defName="MoodOffsetDepressive"]/stages</xpath>
		<value>
			<li>
				<label>depressive</label>
				<description>Penalty from 'depressive' trait with antidepressants.</description>
				<baseMoodEffect>-6</baseMoodEffect>
			</li>
		</value>
	</Operation>

	<!-- Relabel Chitchat -->
	<Operation Class="PatchOperationReplace">
		<xpath>*/ThoughtDef[defName="Chitchat"]/stages/li[1]/label</xpath>
		<value>
			<label>Smalltalk</label>
		</value>
	</Operation>

	<!-- CapturedMe opinion offset -->
	<Operation Class="Psychology.PatchOperationImprisonedMe">
		<xpath>*/ThoughtDef[defName="CapturedMe"]/stages/li[1]/baseOpinionOffset/text()</xpath>
	</Operation>

	<!-- RapportBuilt opinion offset -->
	<Operation Class="Psychology.PatchOperationRapportBuilt">
		<xpath>*/ThoughtDef[defName="RapportBuilt"]/stages/li[1]/baseOpinionOffset/text()</xpath>
	</Operation>

